{
  "name": "vite-react-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "start": "npm run start:development",
    "start:development": "vite --mode development",
    "start:staging": "vite --mode staging",
    "start:production": "vite --mode production",
    "build": "npm run build:development",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" && eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "storybook:build": "sb build",
    "storybook:start": "sb dev -p 6006",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/inputmask": "^5.0.3",
    "@types/invariant": "^2.2.35",
    "@xstate/react": "^3.2.1",
    "axios": "^1.3.5",
    "bootstrap": "^5.3.0-alpha2",
    "bootstrap-icons": "^1.10.4",
    "formik": "^2.2.9",
    "immer": "^9.0.21",
    "inputmask": "^5.0.8",
    "inputmask.phone": "^1.0.5",
    "invariant": "^2.2.4",
    "libphonenumber-js": "^1.10.26",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-select": "^5.7.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "storybook": "^7.0.2",
    "type-fest": "^3.8.0",
    "use-immer": "^0.9.0",
    "xstate": "^4.37.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.37.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/webrtc": "^0.0.34",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-recommended": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.61.0",
    "storybook-dark-mode": "^3.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-twbs-bootstrap": "^8.0.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-stylelint": "^4.3.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8",
    "workbox-window": "^6.5.4"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
