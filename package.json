{
  "name": "vite-react-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "start": "npm run start:development",
    "start:development": "vite --mode development",
    "start:staging": "vite --mode staging",
    "start:production": "vite --mode production",
    "build": "npm run build:development",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" && eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "storybook:build": "sb build",
    "storybook:start": "sb dev -p 6006",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/inputmask": "^5.0.3",
    "@types/invariant": "^2.2.35",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "formik": "^2.4.4",
    "immer": "^10.0.2",
    "inputmask": "^5.0.8",
    "inputmask.phone": "^1.0.5",
    "invariant": "^2.2.4",
    "libphonenumber-js": "^1.10.28",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "redux": "^4.2.1",
    "storybook": "^7.4.0",
    "type-fest": "^4.3.1",
    "use-immer": "^0.9.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.44.2",
    "@types/jsdom": "^21.1.2",
    "@types/lodash-es": "^4.17.9",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webrtc": "^0.0.36",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.34.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-recommended": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^6.0.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.66.1",
    "storybook-dark-mode": "^3.0.1",
    "stylelint": "^15.10.3",
    "stylelint-config-twbs-bootstrap": "^11.0.1",
    "typescript": "^5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-stylelint": "^5.1.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
